        "imagemin-pngcrush": "^6.0.0",
        "imagemin-webpack-plugin": "^2.4.2",
        "jquery": "^3.4.1",
        "laravel-mix": "^4.0.15",
        "lodash": "^4.17.5",
        "minifier": "^0.8.1",
        "popper.js": "^1.12",
        "purify-css": "^1.2.5",
        "purifycss-webpack": "^0.7.0"
